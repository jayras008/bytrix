{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "APPWRITE_ENDPOINT": "@appwrite-endpoint",
    "APPWRITE_PROJECT_ID": "@appwrite-project-id",
    "APPWRITE_API_KEY": "@appwrite-api-key",
    "APPWRITE_BUCKET_ID": "private-files",
    "API_KEY": "@api-key"
  }
}
